#define macro GET_SBOX_VALUE(sbox_compressed_idx) = takes(1) returns (1) {
    pop <sbox_compressed_idx> swap1 0x10 // [0x10, index_in_compressed_sbox, sbox_0]
    UNPACK_FROM_CENTER() // [sbox_value]
}

#define fn SBOX() = takes(1) returns(1) {
    // TODO: optimize into macro, this is disgusting
    // get sbox value from index provided by top element
    // of stack
    // jumpi while keeping result in the top of the stack
    // input stack: [index]
    dup1 0x10 swap1 div // [index_of_compressed_sbox, index]
    swap1 0x10 swap1 mod // [index_in_compressed_sbox, index_of_compressed_sbox]
    0x10 mul swap1 // [index_of_compressed_sbox, index_in_compressed_sbox]

    // now we need to compare the value of the index of compressed sbox
    // with the index of the sbox we want to fetch
    // ifeq 0 => 42
    dup1 0x00 eq found_compressed_sbox_index_0 jumpi
    dup1 0x01 eq found_compressed_sbox_index_1 jumpi
